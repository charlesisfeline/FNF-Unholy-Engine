config/tags=PackedStringArray("audio")
run/main_scene="res://game/scenes/menus/title_scene.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.png"

[autoload]

Prefs="*res://game/backend/Prefs.gd"
Audio="*res://game/backend/Audio.gd"
Game="*res://game/Game.gd"
Conductor="*res://game/backend/Conductor.gd"
HighScore="*res://game/backend/HighScore.gd"
DebugInfo="*res://game/debug_info.tscn"
