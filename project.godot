[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Unholy Engine"

[editor]

movie_writer/movie_file="C:/Users/Jaeden/Downloads/Ay yo codin/Godot v4.2.2/Unholy Engine/wargh.avi"
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-rpc-gd/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "red",
"res://game/": "green",
"res://game/resources/": "blue",
"res://game/tools/": "yellow"
}

